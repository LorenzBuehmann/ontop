[PrefixDeclaration]
obda:		https://w3id.org/obda/vocabulary#
xsd:		http://www.w3.org/2001/XMLSchema#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	mapping-549884572
target		_:{deptId} <http://example.com/dept#deptno> {"deptno"}^^xsd:positiveInteger ; <http://example.com/dept#name> {"dname"} ; <http://example.com/dept#COMPANY> "EXAMPLE Corporation" . 
source		Select ('Department' || "deptno") AS deptId
			            , "deptno"
			            , "dname"
			            , "loc"
			         from "DEPT"

mappingId	mapping--352455519
target		<http://example.com/emp/{"empno"}> a <http://example.com/dept#Manager> ; <http://example.com/emp#jobtype> <http://example.com/emp/job/{"job"}> ; <http://example.com/emp#job> {"job"} ; rdfs:label {"Name"}@en . 
source		SELECT * FROM "EMP"

mappingId	tm-join--382605221
target		<http://example.com/emp/{"CHILD_empno"}> <http://example.com/emp#c_ref_deptno> _:{PARENT_deptId} . 
source		SELECT CHILD."empno" AS "CHILD_empno", PARENT.deptId AS PARENT_deptId FROM (SELECT * FROM "EMP") CHILD, (
			       Select ('Department' || "deptno") AS deptId
			            , "deptno"
			            , "dname"
			            , "loc"
			         from "DEPT"
			       ) PARENT WHERE CHILD."deptno" = PARENT."deptno"
]]

